{
  "name": "web-bridge-template",
  "description": "Extend web-bridge-base template",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./containerApp": {
      "types": "./dist/containerApp.d.ts",
      "import": "./dist/containerApp.js",
      "require": "./dist/containerApp.cjs"
    },
    "./microApp": {
      "types": "./dist/microApp.d.ts",
      "import": "./dist/microApp.js",
      "require": "./dist/microApp.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepublish": "npm run build",
    "pub": "npm publish"
  },
  "devDependencies": {
    "typescript": "~5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.0"
  },
  "dependencies": {
    "web-bridge-base": "^1.0.5"
  }
}
